    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="value" type="xsd:string" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="IntArray">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="value" type="xsd:int" />
    </xsd:sequence>
